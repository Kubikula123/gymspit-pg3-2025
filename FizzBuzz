int num;
Console.WriteLine("Enter a number:");
num = int.Parse(Console.ReadLine());

for (int i = 0;i <= num; i++)
{
    if (i % 3 == 0 && i % 5 == 0)
    {
        Console.WriteLine("Fizz Buzz ");
    }
    else if (i % 3 == 0) 
    {
        Console.WriteLine("Fizz ");
    }
    else if (i % 5 == 0)
    {
        Console.WriteLine("Buzz ");
    }
    else
    {
        Console.WriteLine(i);
    }
}
